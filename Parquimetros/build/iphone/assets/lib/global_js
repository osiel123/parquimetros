function showMessage(e){Ti.UI.createAlertDialog({message:e,ok:L("ok"),title:L("attention")}).show()}function showMessageError(e){Ti.UI.createAlertDialog({message:e,ok:L("ok"),title:L("unexpected_error")}).show()}function isInt(e){return!isNaN(e)&&parseInt(Number(e))==e&&!isNaN(parseInt(e,10))}function validateRFC(e){var t=/[A-Z]{3,4}[0-9]{6}[A-Z0-9]{3}/;return t.test(e)}function validateEmail(e){var t=/^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;return t.test(e)}function removeChildrens(e){for(i in e.children){var t=e.children[0];removeChildrens(t),e.remove(t),t=null}}