function MenuView(){var e=require("/lib/network"),t=require("/lib/database");viewMenuContainer=Ti.UI.createView({left:NaN,isvisible:!1,touchEnabled:!0,width:"100%",height:Ti.UI.FILL,backgroundColor:"transparent",top:"60dp",left:"-200%"}),viewBackgroundMenu=Ti.UI.createView({top:0,height:Ti.UI.FILL,width:Ti.UI.FILL,touchEnabled:!0,backgroundColor:"black",opacity:0}),viewMenu=Ti.UI.createView({top:0,width:"80%",height:Ti.UI.FILL,left:0,backgroundColor:"white",borderColor:"#d3d3d3"}),tableViewMenu=Ti.UI.createTableView({top:0,left:0,width:Ti.UI.FILL,height:Ti.UI.SIZE,backgroundColor:Ti.App.tableRowBackgroundColor,separatorColor:"transparent"});var i=function(e){var t=Ti.UI.createTableViewRow({id:e.id,height:"55dp",width:Ti.UI.FILL,color:Ti.App.tableRowLabelColor,backgroundSelectedColor:Ti.App.selectedColor,backgroundColor:Ti.App.tableRowBackgroundColor}),i=Ti.UI.createImageView({image:e.image,touchEnabled:!1,bubbleParent:!0,left:0,width:"45dp",height:"45dp"}),o=Ti.UI.createLabel({color:Ti.App.labelColor,left:"55dp",width:Ti.UI.SIZE,height:Ti.UI.SIZE,text:e.title,font:{fontSize:Ti.App.fontSize}});return viewLineRow=Titanium.UI.createView({width:"90%",height:1,backgroundColor:Ti.App.tableRowSeparatorColor,bottom:0}),t.add(o),t.add(i),t.add(viewLineRow),t};return rows=[],rows.push(i({id:1,title:L("reprint"),image:"/images/reprint_icon.png"})),rows.push(i({id:9,title:L("out"),image:"/images/close_icon.png"})),tableViewMenu.addEventListener("click",function(i){viewMenu.enabled=!1,0==i.rowData.id?Ti.App.fireEvent("hide_menu"):1==i.rowData.id?(Ti.App.fireEvent("hide_menu"),GetTicketModalWindow=require("/ui/GetTicketModalWindow"),new GetTicketModalWindow({}).open()):9==i.rowData.id&&(Ti.App.fireEvent("hide_menu"),e.signOut(function(e){t.deleteUser(function(e){Ti.App.fireEvent("close_app")})})),setTimeout(function(){viewMenu.enabled=!0},2e3)}),viewBackgroundMenu.addEventListener("click",function(e){Ti.App.fireEvent("hide_menu")}),tableViewMenu.setData(rows),viewMenu.add(tableViewMenu),viewMenuContainer.add(viewBackgroundMenu),viewMenuContainer.add(viewMenu),viewMenuContainer}module.exports=MenuView;